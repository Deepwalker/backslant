!doctype/(html)
html
    head
        - a = 'pam'
        title(a="a")
            - yield options.get('title', 'empty')
        style """
            there
        """
    body
        h1
            - yield options.get('title', 'empty')
        div.content
            - content = options.get('content')
            - if content:
                - yield from content()
